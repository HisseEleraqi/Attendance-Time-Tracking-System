@model AttendenceSystem.Models.Schedule




@{
    string TrackName = ViewData["TrackName"] as string;
    ViewData["Title"] = "Details";
}

<h1>Trach: @TrackName</h1>
<h4>Schedule Details</h4>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Date
        </dt>
        <dd class = "col-sm-10">
            @Model.Date
        </dd>
        <dt class = "col-sm-2">
            StartTime
        </dt>
        <dd class = "col-sm-10">
            @Model.StartTime
        </dd>
        <dt class = "col-sm-2">
            EndTime
        </dt>
        <dd class = "col-sm-10">
            @Model.EndTime
        </dd>
        <dt class = "col-sm-2">
            Tracks
        </dt>
        <dd class = "col-sm-10">
            @Model.Tracks.Id
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
@section SideBar {

    <li>
        <a href="/Permissions">Permissions</a>
    </li>
    <li>
        <a href="/Schedules">Schedule</a>
    </li>

}